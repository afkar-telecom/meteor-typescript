  "version": "0.10.6",
  "license": "MIT",
  "description": "TypeScript wrapper package for use with Meteor",
  "keywords": [
    "meteor",
    "typescript",
    "es6",
    "es7",
    "transpiler",
    "transpilation",
    "compilation"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
