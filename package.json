    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "jasmine-node": "^1.14.5",
